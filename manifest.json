{
	"name": "Verified NFT",
	"version": "1.1",
	"description": "Making sure your NFT is verified so you won't get scammed",
	"manifest_version": 3,
	"incognito": "split",
	"icons": {
		"128": "icons/128.png"
	},
    "action": {
		"default_icon": "icons/128.png",
		"default_popup": "popup/popup.html"
	},
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
      "*://*.opensea.io/*"
     ]
     
	
}

